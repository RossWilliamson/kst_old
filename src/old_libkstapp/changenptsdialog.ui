    <tabstop>Clear</tabstop>
    <tabstop>SelectAll</tabstop>
    <tabstop>_kstDataRange</tabstop>
    <tabstop>OK</tabstop>
    <tabstop>Cancel</tabstop>
</tabstops>
<includes>
