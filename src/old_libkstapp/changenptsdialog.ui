        <slot>reject()</slot>
    </connection>
    <connection>
        <sender>CurveList</sender>
        <signal>selectionChanged()</signal>
        <receiver>KstChangeNptsDialog</receiver>
        <slot>updateTimeCombo()</slot>
    </connection>
</connections>
<tabstops>
